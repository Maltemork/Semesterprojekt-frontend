export function constructSponsorObject(sponsordata) {
    const sponsorObject = {
        id: sponsordata.id,
        number: sponsordata.trackNo,
        title: sponsordata.title,
        album: sponsordata.albumName,
        artistName: sponsordata.artistName,
        releaseDate: sponsordata.releaseDate,
        duration: sponsordata.duration
    }

    return sponsorObject;
}